# This file is for unifying the coding style for different editors and IDEs
# editorconfig.org


[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

# Use 2 spaces in JavaScript, JSON and text files.
[**.{js,json,txt,md}]
indent_style = spaces
indent_size = 2
